{
  "name": "@paulshryock/personal-website",
  "version": "0.1.2",
  "private": true,
  "description": "Paul Shryock's personal website.",
  "homepage": "https://github.com/paulshryock/personal-website#readme",
  "bugs": {
    "url": "https://github.com/paulshryock/personal-website/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulshryock/personal-website.git"
  },
  "license": "MIT",
  "author": "Paul Shryock <paul@pshry.com> (https://github.com/paulshryock)",
  "type": "module",
  "scripts": {
    "build": "run-s compile test:acceptance",
    "bump": "release-bump -epq",
    "clean": "rm -rf dist",
    "precompile": "npm run clean",
    "compile": "ts-node ./bin/compile.ts",
    "deploy": "netlify deploy",
    "format": "prettier --loglevel=warn --write \"./**/*.{{t,j}s,{m,c}{t,j}s,json}\"",
    "lint": "run-p lint:*",
    "lint:css": "npx stylelint \"src/scss/**/*.scss\" --fix",
    "lint:js": "eslint \"./**/*.{ts,d.ts,js,{m,c}js,json}\" --config=eslint.config.cjs --fix",
    "lint:pkg": "sort-package-json",
    "lint:yaml": "yaml-validator .github/workflows/*.yml",
    "prepare": "husky install bin/husky",
    "prerelease": "touch .env && source .env",
    "quality": "npm-run-all --serial format --parallel lint typecheck test:unit",
    "preserve": "npm run precompile",
    "serve": "BUILD_ENV=local BUILD_SERVE=true npm run compile",
    "start": "npm run serve",
    "test": "jest",
    "test:acceptance": "npm test -- acceptance --coverage=false",
    "test:unit": "npm test -- --coverage unit",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:watch": "npm test -- --watch",
    "typecheck": "tsc",
    "typecheck:watch": "npm run typecheck -- --watch",
    "upgrade:all": "ncu --upgrade",
    "upgrade:dry-run": "ncu",
    "upgrade:minor": "ncu --target=minor --upgrade",
    "upgrade:patch": "ncu --target=patch --upgrade",
    "preversion": "if [[ $(git status -s) ]]; then exit 1; fi",
    "version": "npm run bump && git add ."
  },
  "browserslist": [
    "defaults"
  ],
  "devDependencies": {
    "@11ty/eleventy": "2.0.2-alpha.2",
    "@swc/core": "1.3.70",
    "@testing-library/dom": "8.20.1",
    "@testing-library/jest-dom": "5.17.0",
    "@types/jest": "28.1.8",
    "@types/jsdom": "16.2.15",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "autoprefixer": "10.4.14",
    "cssnano": "5.1.15",
    "dotenv": "16.3.1",
    "esbuild": "0.14.54",
    "esbuild-plugin-browserslist": "0.4.13",
    "eslint": "8.45.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-json-format": "2.0.1",
    "html-minifier": "4.0.0",
    "husky": "8.0.3",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.3",
    "jest-ts-webcompat-resolver": "1.0.0",
    "mock-match-media": "0.4.3",
    "node-html-parser": "5.4.2",
    "npm-check-updates": "15.3.4",
    "npm-run-all2": "6.0.6",
    "postcss": "8.4.27",
    "postcss-advanced-variables": "3.0.1",
    "postcss-easy-import": "4.0.0",
    "postcss-nested": "5.0.6",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.8",
    "release-bump": "3.0.0-alpha.69",
    "sass": "1.64.1",
    "sort-package-json": "1.57.0",
    "stylelint": "14.9.1",
    "stylelint-config-standard-scss": "4.0.0",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "yaml-validator": "4.0.1"
  }
}
